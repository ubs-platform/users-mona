  "sourceRoot": "libs/users-microservice-helper/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/users-microservice-helper",
        "tsConfig": "libs/users-microservice-helper/tsconfig.lib.json",
